Tuto6_for{

	void ajouterUn(int[] tab){
		for(int i=0; i<tab.length; i++){
			tab[i]=1;	
		}	
	}
	
	void afficher(int[] tab){
      for(int i=0 ; i<tab.length; i++){
         println(tab[i]+"");
      }
   }

	
	
 	void main(){
		
	   	int[] tab = new int[10];
		ajouterUn(tab);
      	afficher(tab);
   }
}