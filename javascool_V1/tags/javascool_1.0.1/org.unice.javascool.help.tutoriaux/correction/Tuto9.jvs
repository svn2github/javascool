Tuto9{
	
	
	void main(){
		int dec = 128; //le nombre decimal a convertir en binaire
		
		int[] res = new int[8];
		int i=7;
		while(dec>0){
			int reste = dec%2;
			res[i] = reste;
			dec = dec/2;
			i--;			
		}		
		
		//affichage du resultat
		for(int j=0; j<res.length; j++){
			print(res[j]+" ");	
		}
		
	}
}