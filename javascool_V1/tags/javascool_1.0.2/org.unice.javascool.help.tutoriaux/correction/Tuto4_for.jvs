Tuto4_for{
	
	String lirePrenom(){
		String prenom = readString();
		return prenom;	
	}
	
	int lireAge(){
		int age = readInt();
		return age;
	}
	
	void main(){
		for(int i =0; i<3; i++){
			println("entrez votre prenom : ");
			String prenom = lirePrenom();
			println("entrez votre age : ");
			int age = lireAge();	
		
			println("prenom : " + prenom);
			println("age : " +age+" ans");
		}
	}
}
