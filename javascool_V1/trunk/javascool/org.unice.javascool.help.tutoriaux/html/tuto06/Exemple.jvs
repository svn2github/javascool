Exemple{

   void afficher(int[] tab){
      for(int i=0 ; i<tab.length; i++){
         println(tab[i]+"");
      }
   }


   void main(){
      int[] tab = new int[8];
      tab[0] = 1;
      tab[1] = 2;
      tab[2] = 3;
      tab[3] = 4;
      tab[4] = 5;
      tab[5] = 6;
      tab[6] = 7;
      tab[7] = 8;
      afficher(tab);
   }
}