<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="%currencyeditor.EDITOR_NAME"
            extensions="jvs,java"
            icon="img/sample.gif"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            class="org.javascool.ui.editor.editors.JVSEditor"
            id="org.javascool.ui.editor.editors.JVSEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.javascool.ui.editor.Perspective"
            icon="img/sample.gif"
            id="org.javascool.ui.editor.perspectiveEditor"
            name="JVS Editor">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.javascool.ui.editor.preferences.ColorPreferencePage"
            id="org.javascool.ui.editor.preferences.ColorPreferencePage"
            name="Coloration syntaxique">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.javascool.ui.editor.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.javascool.ui.editor.actionSet1"
            label="label"
            visible="true">
         <action
               class="org.javascool.ui.editor.actions.OpenFileAction"
               icon="img/charger.png"
               id="org.unice.javascool.actions.OpenFileAction"
               label="Ouvrir un fichier..."
               style="push"
               toolbarPath="main/FileGroup"
               tooltip="Ouvrir un fichier">
         </action>
         <action
               class="org.javascool.ui.editor.actions.OpenFileAction"
               definitionId="org.javascool.ui.editor.OpenFileCommand"
               icon="img/charger_16px.png"
               id="org.unice.javascool.actions.OpenFileActionMenu"
               label="Ouvrir un fichier..."
               menubarPath="file/fileStart"
               style="push"
               tooltip="Ouvrir un fichier">
         </action>
         <action
               class="org.javascool.ui.editor.actions.NewEditorAction"
               icon="img/nouveau.png"
               id="org.javascool.ui.editor.actions.newEditorAction"
               label="Nouveau"
               style="push"
               toolbarPath="main/FileGroup"
               tooltip="Nouveau Fichier">
         </action>
         <action
               class="org.javascool.ui.editor.actions.NewEditorAction"
               definitionId="org.javascool.ui.editor.newEditorCommand"
               icon="img/nouveau_16px.png"
               id="org.javascool.ui.editor.actions.newEditorActionMenu"
               label="Nouveau"
               menubarPath="file/fileStart"
               style="push"
               tooltip="Nouveau Fichier">
         </action>
         <action
               class="org.javascool.ui.editor.actions.CompilCodeAction"
               icon="img/compile.png"
               id="org.javascool.ui.editor.actions.compileCodeAction"
               label="Compilation"
               style="push"
               toolbarPath="console/compil"
               tooltip="Compiler le code Source">
         </action>
         <action
               class="org.javascool.ui.editor.actions.ExecuteCodeAction"
               icon="img/Execute.gif"
               id="org.javascool.ui.editor.actions.executeCodeAction"
               label="Execution"
               style="push"
               toolbarPath="console/execute"
               tooltip="Executer le programme">
         </action>
         <action
               class="org.javascool.ui.editor.actions.StopExecutionAction"
               icon="img/stop.gif"
               id="org.javascool.ui.editor.actions.stopAction"
               label="Stop execution"
               style="push"
               toolbarPath="console/stop"
               tooltip="Arreter le programme en cours d&apos;execution">
         </action>
         <menu
               id="compile"
               label="Source">
         </menu>
         <action
               class="org.javascool.ui.editor.actions.CompilCodeAction"
               definitionId="org.javascool.ui.editor.commandCompil"
               icon="img/compile_16px.png"
               id="org.javascool.ui.editor.actions.compileCodeActionMenu"
               label="Compilation"
               menubarPath="compile/compil"
               style="push"
               tooltip="Compiler le code Source">
         </action>
         <action
               class="org.javascool.ui.editor.actions.ExecuteCodeAction"
               definitionId="org.javascool.ui.editor.commandExecution"
               icon="img/Execute_16px.gif"
               id="org.javascool.ui.editor.actions.executeCodeActionMenu"
               label="Execution"
               menubarPath="compile/execute"
               style="push"
               tooltip="Executer le programme">
         </action>
         <action
               class="org.javascool.ui.editor.actions.StopExecutionAction"
               definitionId="org.javascool.ui.editor.commandStop"
               icon="img/stop_16px.gif"
               id="org.javascool.ui.editor.actions.stopActionMenu"
               label="Stop execution"
               menubarPath="compile/stop"
               style="push"
               tooltip="Arreter le programme en cours d&apos;execution">
         </action>
         <action
               class="org.javascool.ui.editor.actions.ShowJavaSourceAction"
               icon="img/source.gif"
               id="org.javascool.ui.editor.actions.showJavaSourceAction"
               label="Afficher le code source Java"
               menubarPath="compile/Group2"
               style="push"
               tooltip="afficher le code source java">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.javascool.ui.editor.OpenFileCommand"
            name="Ouvrir un Fichier">
      </command>
      <command
            id="org.javascool.ui.editor.newEditorCommand"
            name="Nouveau">
      </command>
      <command
            id="org.javascool.ui.editor.commandCompil"
            name="Compilation">
      </command>
      <command
            id="org.javascool.ui.editor.commandExecution"
            name="Execution">
      </command>
      <command
            id="org.javascool.ui.editor.commandStop"
            name="Stop Execution">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <scheme
            id="org.javascool.ui.editor.scheme"
            name="org.javascool.ui.editor.scheme">
      </scheme>
      <key
            commandId="org.javascool.ui.editor.OpenFileCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+O">
      </key>
      <key
            commandId="org.javascool.ui.editor.newEditorCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+N">
      </key>
      <key
            commandId="org.javascool.ui.editor.commandCompil"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+U">
      </key>
      <key
            commandId="org.javascool.ui.editor.commandExecution"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="m1+T">
      </key>
      <key
            commandId="org.javascool.ui.editor.commandStop"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+K">
      </key>
      <key
            commandId="org.eclipse.ui.edit.undo"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+Z">
      </key>
      <key
            commandId="org.eclipse.ui.edit.redo"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="m1+Y">
      </key>
      <key
            commandId="org.eclipse.ui.edit.cut"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+X">
      </key>
      <key
            commandId="org.eclipse.ui.edit.copy"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="m1+C">
      </key>
      <key
            commandId="org.eclipse.ui.edit.paste"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+V">
      </key>
      <key
            commandId="org.eclipse.ui.edit.delete"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="DEL">
      </key>
      <key
            commandId="org.eclipse.ui.edit.selectAll"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+A">
      </key>
      <key
            commandId="org.eclipse.ui.edit.findReplace"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+F">
      </key>
      <key
            commandId="org.eclipse.ui.file.close"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+W">
      </key>
      <key
            commandId="org.eclipse.ui.file.closeAll"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+M2+W">
      </key>
      <key
            commandId="org.eclipse.ui.file.save"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+S">
      </key>
      <key
            commandId="org.eclipse.ui.file.print"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+P">
      </key>
      <key
            commandId="org.eclipse.ui.file.exit"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.javascool.ui.editor.scheme"
            sequence="M1+Q">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.javascool.ui.editor.AutoOpenEditorAtStart"></startup>
   </extension>

</plugin>
