double divisionDecimale(int dividende, int diviseur) {
  double quotient;
  if(diviseur == 0) {
    quotient = Double.POSITIVE_INFINITY;
  } else {
    quotient = ((double) dividende) / ((double) diviseur);
  }
  return quotient;
}
void main() {
  println(divisionDecimale(2, 3));
}
