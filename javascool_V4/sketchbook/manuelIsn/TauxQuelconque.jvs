void main() {
  double ht, ttc, taux;
  println("Quel est le prix hors taxes ?");
  ht = readDouble();
  println("Quel est le taux de TVA ?");
  taux = readDouble();
  ttc = ht + ht * taux / 100;
  print("Le prix toutes taxes comprises est ");
  println(ttc);
}
