
// Calcule le n-ième terme de la suite de Fibonacci (méthode itérative)
int ifib(int n) {
  int n0 = 0;
  int n1 = 1;
  while(n > 0) {
    int n2 = n0 + n1;
    n0 = n1;
    n1 = n2;
    n = n - 1;
  }
  return n0;
}
// Calcule le n-ième terme de la suite de Fibonacci (méthode récursive)
int rfib(int n) {
  if (n > 1) {
    return rfib(n - 1) + rfib(n - 2);
  } else if (n == 1) {
    return 1;
  } else {
    return 0;
  }
}
// Calcul de la suite
void main() {
  for(int n = 0; n <= 40; n++) {
    println("fib(" + n + ") = " + ifib(n) + " == " + rfib(n));
  }
}
