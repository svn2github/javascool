
void main () {
  println("Entrer les valeurs de deux entiers a et b pour calculer leur pgcd :");
  int a = readInteger();
  int b = readInteger();
  int q = a/b;
  int r = a%b;
  println("a = "+a+" b = "+b);
  println(a+" = "+b +" * "+q+" + "+r);
  while(r!=0) {
    a=b;
    b=r;
    q=a/b;
    r=a%b;
    println(a+" = "+b +" * "+q+" + "+r);
  }
  println("pgcd(a,b) = "+b);
  if(b==1) {
    println("Les entiers a et b sont premiers entre eux !");
  }
}
